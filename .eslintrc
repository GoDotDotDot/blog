{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": 0,
    "react/no-danger": 0,
    "operator-linebreak": [
      "error",
      "after",
      { "overrides": { "?": "before", ":": "before" } }
    ],
    "arrow-parens": 0,
    "object-curly-newline": ["error", { "consistent": true }],
    "implicit-arrow-linebreak": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-props-no-spreading": 0
  },
  "globals": {
    "window": true,
    "FormData": true,
    "fetch": true
  },
  "plugins": ["@typescript-eslint", "prettier"]
}
